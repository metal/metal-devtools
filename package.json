{
  "repository": "bryceosterhaus/metal-devtools",
  "scripts": {
    "build": "npm run build:soy && npm run build:jsx",
    "build:soy": "gulp build",
    "build:jsx": "browserify ./src/jsx/AppJSX.js -o ./build/AppJSX.js -t [ babelify --presets [ metal-jsx es2015 ] ]"
  },
  "babel": {
    "presets": [
      "metal-jsx",
      "es2015"
    ]
  },
  "dependencies": {
    "metal-component": "^2.6.1",
    "metal-jsx": "^2.5.9",
    "metal-soy": "^2.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-metal-jsx": "0.0.3",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "eslint-plugin-react": "^6.9.0",
    "gulp-metal": "^1.10.0"
  }
}
